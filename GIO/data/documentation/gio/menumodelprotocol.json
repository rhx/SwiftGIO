pe":"text","text":" in "},{"type":"codeVoice","code":"model"},{"type":"text","text":"."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/MenuModelProtocol\/iterateItemLinks(itemIndex:)","kind":"symbol","type":"topic","url":"\/documentation\/gio\/menumodelprotocol\/iterateitemlinks(itemindex:)"},"doc://GIO/documentation/GIO/MenuModelRef":{"role":"symbol","title":"MenuModelRef","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"MenuModelRef"}],"abstract":[{"type":"codeVoice","code":"GMenuModel"},{"type":"text","text":" represents the contents of a menu — an ordered list of"},{"type":"text","text":" "},{"type":"text","text":"menu items. The items are associated with actions, which can be"},{"type":"text","text":" "},{"type":"text","text":"activated through them. Items can be grouped in sections, and may"},{"type":"text","text":" "},{"type":"text","text":"have submenus associated with them. Both items and sections usually"},{"type":"text","text":" "},{"type":"text","text":"have some representation data, such as labels or icons. The type of"},{"type":"text","text":" "},{"type":"text","text":"the associated action (ie whether it is stateful, and what kind of"},{"type":"text","text":" "},{"type":"text","text":"state it has) can influence the representation of the item."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/MenuModelRef","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"MenuModelRef"}],"url":"\/documentation\/gio\/menumodelref"},"doc://GIO/documentation/GIO/Menu":{"role":"symbol","title":"Menu","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"Menu"}],"abstract":[{"type":"codeVoice","code":"GMenu"},{"type":"text","text":" is a simple implementation of [class"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/MenuModel"},{"type":"text","text":"]."},{"type":"text","text":" "},{"type":"text","text":"You populate a "},{"type":"codeVoice","code":"GMenu"},{"type":"text","text":" by adding [class"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/MenuItem"},{"type":"text","text":"] instances to it."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/Menu","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Menu"}],"url":"\/documentation\/gio\/menu"},"doc://GIO/documentation/GIO/MenuModelProtocol":{"role":"symbol","title":"MenuModelProtocol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"MenuModelProtocol"}],"abstract":[{"type":"codeVoice","code":"GMenuModel"},{"type":"text","text":" represents the contents of a menu — an ordered list of"},{"type":"text","text":" "},{"type":"text","text":"menu items. The items are associated with actions, which can be"},{"type":"text","text":" "},{"type":"text","text":"activated through them. Items can be grouped in sections, and may"},{"type":"text","text":" "},{"type":"text","text":"have submenus associated with them. Both items and sections usually"},{"type":"text","text":" "},{"type":"text","text":"have some representation data, such as labels or icons. The type of"},{"type":"text","text":" "},{"type":"text","text":"the associated action (ie whether it is stateful, and what kind of"},{"type":"text","text":" "},{"type":"text","text":"state it has) can influence the representation of the item."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/MenuModelProtocol","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"MenuModelProtocol"}],"url":"\/documentation\/gio\/menumodelprotocol"},"doc://GIO/documentation/GIO/DBusMenuModelProtocol":{"role":"symbol","title":"DBusMenuModelProtocol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"DBusMenuModelProtocol"}],"abstract":[{"type":"codeVoice","code":"GDBusMenuModel"},{"type":"text","text":" is an implementation of [class"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/MenuModel"},{"type":"text","text":"] that can be"},{"type":"text","text":" "},{"type":"text","text":"used as a proxy for a menu model that is exported over D-Bus with"},{"type":"text","text":" "},{"type":"text","text":"[method"},{"type":"codeVoice","code":"Gio.DBusConnection.export_menu_model"},{"type":"text","text":"]."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/DBusMenuModelProtocol","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"DBusMenuModelProtocol"}],"url":"\/documentation\/gio\/dbusmenumodelprotocol"},"doc://GIO/documentation/GIO/MenuModelProtocol/connect(signal:flags:data:destroyData:signalHandler:)":{"role":"symbol","title":"connect(signal:flags:data:destroyData:signalHandler:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"connect"},{"kind":"text","text":"("},{"kind":"externalParam","text":"signal"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"MenuModelSignalName","preciseIdentifier":"s:3GIO19MenuModelSignalNameO"},{"kind":"text","text":", "},{"kind":"externalParam","text":"flags"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"ConnectFlags","preciseIdentifier":"s:10GLibObject12ConnectFlagsV"},{"kind":"text","text":", "},{"kind":"externalParam","text":"data"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"gpointer","preciseIdentifier":"c:gtypes.h@T@gpointer"},{"kind":"text","text":"!, "},{"kind":"externalParam","text":"destroyData"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"GClosureNotify","preciseIdentifier":"c:@T@GClosureNotify"},{"kind":"text","text":"?, "},{"kind":"externalParam","text":"signalHandler"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"GCallback","preciseIdentifier":"c:gclosure.h@T@GCallback"},{"kind":"text","text":") -> "},{"kind":"typeIdentifier","text":"Int