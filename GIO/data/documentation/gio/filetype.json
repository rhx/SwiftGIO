and that"},{"type":"text","text":" "},{"type":"codeVoice","code":"GFile"},{"type":"text","text":" objects do not represent files, merely an identifier for a"},{"type":"text","text":" "},{"type":"text","text":"file. All file content I\/O is implemented as streaming operations"},{"type":"text","text":" "},{"type":"text","text":"(see [class"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/InputStream"},{"type":"text","text":"] and [class"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/OutputStream"},{"type":"text","text":"])."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/File","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"File"}],"url":"\/documentation\/gio\/file"},"doc://GIO/documentation/GIO/FileInfo":{"role":"symbol","title":"FileInfo","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"FileInfo"}],"abstract":[{"type":"text","text":"Stores information about a file system object referenced by a [iface"},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/File"},{"type":"text","text":"]."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/FileInfo","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"FileInfo"}],"url":"\/documentation\/gio\/fileinfo"},"doc://GIO/documentation/GIO/FileType":{"role":"symbol","title":"FileType","fragments":[{"kind":"keyword","text":"typealias"},{"kind":"text","text":" "},{"kind":"identifier","text":"FileType"}],"abstract":[{"type":"text","text":"Indicates the file’s on-disk type."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/FileType","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"FileType"}],"url":"\/documentation\/gio\/filetype"},"doc://GIO/documentation/GIO":{"role":"collection","title":"GIO","abstract":[],"identifier":"doc:\/\/GIO\/documentation\/GIO","kind":"symbol","type":"topic","url":"