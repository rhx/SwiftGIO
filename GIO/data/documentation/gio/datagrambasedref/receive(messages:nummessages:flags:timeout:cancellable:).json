asedRef"}],"abstract":[{"type":"text","text":"Interface for socket-like objects with datagram semantics."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/DatagramBasedRef","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"DatagramBasedRef"}],"url":"\/documentation\/gio\/datagrambasedref"},"doc://GIO/documentation/GIO/DatagramBasedRef/receive(messages:numMessages:flags:timeout:cancellable:)":{"role":"symbol","title":"receive(messages:numMessages:flags:timeout:cancellable:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"receive"},{"kind":"text","text":"("},{"kind":"externalParam","text":"messages"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UnsafeMutablePointer","preciseIdentifier":"s:Sp"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"GInputMessage"},{"kind":"text","text":">!, "},{"kind":"externalParam","text":"numMessages"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"},{"kind":"text","text":", "},{"kind":"externalParam","text":"flags"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"},{"kind":"text","text":", "},{"kind":"externalParam","text":"timeout"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"gint64","preciseIdentifier":"c:glibconfig.h@T@gint64"},{"kind":"text","text":", "},{"kind":"externalParam","text":"cancellable"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"CancellableRef","preciseIdentifier":"s:3GIO14CancellableRefV"},{"kind":"text","text":"?) "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"}],"abstract":[{"type":"text","text":"Receive one or more data messages from "},{"type":"codeVoice","code":"datagramBased"},{"type":"text","text":" in one go."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/DatagramBasedRef\/receive(messages:numMessages:flags:timeout:cancellable:)","kind":"symbol","type":"topic","url":"\/documentation\/gio\/datagrambasedref\/receive(messages:nummessages:flags:timeout:cancellable:)"},"doc://GIO/documentation/GIO/DatagramBasedRef/DatagramBasedProtocol-Implementations":{"role":"collectionGroup","title":"DatagramBasedProtocol Implementations","abstract":[],"identifier":"doc:\/\/GIO\/documentation\/GIO\/DatagramBasedRef\/DatagramBasedProtocol-Implementations","kind":"article","type":"topic","url":"\/documentation\/gio\/datagrambasedref\/datagrambasedprotocol-implementations"},"doc://GIO/documentation/GIO/InputVector":{"role":"symbol","title":"InputVector","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"InputVector"}],"abstract":[{"type":"text","text":"Structure used for scatter\/gather data input."},{"type":"text","text":" "},{"type":"text","text":"You generally pass in an array of "},{"type":"codeVoice","code":"GInputVectors"},{"type":"text","text":" "},{"type":"text","text":"and the operation will store the read data starting in the"},{"type":"text","text":" "},{"type":"text","text":"first buffer, switching to the next as needed."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/InputVector","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"InputVector"}],"url":"\/documentation\/gio\/inputvector"},"doc://GIO/documentation/GIO/InputMessage":{"role":"symbol","title":"InputMessage","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"InputMessage"}],"abstract":[{"type":"text","text":"Structure used for scatter\/gather data input when receiving multiple"},{"type":"text","text":" "},{"type":"text","text":"messages or packets in one go. You generally pass in an array of empty"},{"type":"text","text":" "},{"type":"codeVoice","code":"GInputVectors"},{"type":"text","text":" and the operation will use all the buffers as if they"},{"type":"text","text":" "},{"type":"text","text":"were one buffer, and will set "},{"type":"codeVoice","code":"bytes_received"},{"type":"text","text":" to the total number of bytes"},{"type":"text","text":" "},{"type":"text","text":"received across all "},{"type":"codeVoice","code":"GInputVectors"},{"type":"text","text":"."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/InputMessage","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"InputMessage"}],"url":"\/documentation\/gio\/inputmessage"},"doc://GIO/documentation/GIO/SocketMsgFlags":{"role":"symbol","title":"SocketMsgFlags","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"SocketMsgFlags"}],"abstract":[{"type":"text","text":"Flags used in "},{"type":"codeVoice","code":"g_socket_receive_message()"},{"type":"text","text":" and "},{"type":"codeVoice","code":"g_socket_send_message()"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"The flags listed in the enum are some commonly available flags, but the"},{"type":"text","text":" "},{"type":"text","text":"values used for them are the same as on the platform, and any other flags"},{"type":"text","text":" "},{"type":"text","text":"are passed in\/out as is. So to use a platform specific flag, just include"},{"type":"text","text":" "},{"type":"text","text":"the right system header and pass in the flag."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/SocketMsgFlags","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SocketMsgFlags"}],"url":"\/documentation\/gio\/socketmsgflags"}}}